  "inputs": [
    {
      "id": "pathToSpec",
      "type": "promptString",
      "description": "Enter the path to the spec file",
      "default": "${workspaceFolder}/examples/hello_world.yaml"
    },
    {
      "id": "runId",
      "type": "promptString",
      "description": "Enter the run id",
      "default": ""
    },
  ]
