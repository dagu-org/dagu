    {
      "name": "Status",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/",
      "args": [
        "status",
        "${input:pathToSpec}"
      ]
    },
    {
      "name": "Enqueue",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/",
      "args": [
        "enqueue",
        "${input:pathToSpec}"
      ]
    },
    {
      "name": "Migrate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/",
      "args": [
        "migrate",
        "history"
      ]
