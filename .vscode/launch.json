    "version": "0.2.0",
    "configurations": [
        {
            "name": "Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "server",
                "--port=8080",
            ]
        },
        {
            "name": "Scheduler",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "scheduler",
            ]
        },
        {
            "name": "Start",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "start",
                "${input:pathToSpec}"
            ]
        },
        {
            "name": "Test",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "start",
                "--root=_parent:0196b021-11a9-78ba-ae6e-e2b78eda65c5",
                "--parent=_parent:0196b021-11a9-78ba-ae6e-e2b78eda65c5",
                "--exec-id=0196b021-11bf-742a-98fb-b42919195327",
                "/Users/hamadayouta/dev/dagu/.local/dags/_child.yaml"
            ]
        }
    ],
    "inputs": [
        {
            "id": "pathToSpec",
            "type": "promptString",
            "description": "Enter the path to the spec file",
            "default": "${workspaceFolder}/examples/hello_world.yaml"
        }
    ]
}