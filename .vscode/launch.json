{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "server",
                "--port=8080",
                "-s",
                "localhost"
            ]
        },
        {
            "name": "Scheduler",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "scheduler",
            ]
        },
        {
            "name": "Start",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "start",
                "${input:pathToSpec}"
            ]
        },
        {
            "name": "StartSub",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/",
            "args": [
                "start",
                "--request-id=04a60fa6-adfd-4f52-b2e3-a4a5d24a1861",
                "--root-dag-name=_parent",
                "--root-request-id=863617f9-f48f-4359-bf73-1ee9fb0d155e",
                "--quiet",
                "/Users/hamadayouta/dev/dagu/.local/dags/_child.yaml"
            ]
        }
    ],
    "inputs": [
        {
            "id": "pathToSpec",
            "type": "promptString",
            "description": "Enter the path to the spec file",
            "default": "${workspaceFolder}/examples/hello_world.yaml"
        }
    ]
}